    "@types/node": "18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "aws-cdk": "^2.55.0",
    "eslint": "^8.29.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.55.0",
    "constructs": "^10.1.190",
